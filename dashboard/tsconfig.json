{
  "compilerOptions": {
    "target": "ES2022",
    "lib": ["dom", "es2022"],
    "jsx": "preserve",
    "module": "esnext",
    "moduleResolution": "bundler",
    "strict": false,
    "skipLibCheck": true,
    "allowJs": false,
    "resolveJsonModule": true,
    "noEmit": true
  },
  "include": ["app/**/*"]
}
